<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
  <description><![CDATA["封装了工作流引擎的一下功能：如流转历史"]]></description>
  <display-name>"工作流引擎标签"</display-name>
  <tlib-version>1.0</tlib-version>
  <short-name>wf</short-name>
  <uri>http://www.norteksoft.com/workflow/tags</uri>
  <tag>
    <description><![CDATA[显示流转历史]]></description>
    <name>history</name>
    <tag-class>com.norteksoft.tags.workflow.WorkflowHistoryTag</tag-class>
    <body-content>tagdependent</body-content>
    <attribute>
      <description><![CDATA[流转历史的swf文件放置位置]]></description>
      <name>url</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[公司id]]></description>
      <name>companyId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[任务名称]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[显示附件上传页面]]></description>
    <name>upload</name>
    <tag-class>com.norteksoft.tags.workflow.UploadAttachmentTag</tag-class>
    <body-content>tagdependent</body-content>
    <attribute>
      <description><![CDATA[公司id]]></description>
      <name>companyId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[上传文件的大小]]></description>
      <name>fileSizeLimit</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[上传文件的类型]]></description>
      <name>fileTypes</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[文件的类型描述]]></description>
      <name>fileTypesDescription</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[文件数目]]></description>
      <name>fileUploadLimit</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[SWF组件的URL]]></description>
      <name>moduleUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[上传按钮图片的URL]]></description>
      <name>imageUrl</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[显示意见页面]]></description>
    <name>opinion</name>
    <tag-class>com.norteksoft.tags.workflow.OpinionTag</tag-class>
    <body-content>tagdependent</body-content>
    <attribute>
      <description><![CDATA[公司id]]></description>
      <name>companyId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[表单打印权限]]></description>
    <name>printRights</name>
    <tag-class>com.norteksoft.tags.workflow.TablePrintRightsTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[流程删除权限]]></description>
    <name>deleteRights</name>
    <tag-class>com.norteksoft.tags.workflow.DeleteRightsTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
  	<description><![CDATA[查看会签结果]]></description>
  	<name>counterSign</name>
  	<tag-class>com.norteksoft.tags.workflow.CountersignResultTag</tag-class>
  	<body-content>tagdependent</body-content>
  	<attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
  	<description><![CDATA[查看投票结果]]></description>
  	<name>vote</name>
  	<tag-class>com.norteksoft.tags.workflow.VoteResultTag</tag-class>
  	<body-content>tagdependent</body-content>
  	<attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[显示正文页面]]></description>
    <name>official</name>
    <tag-class>com.norteksoft.tags.workflow.OfficialTag</tag-class>
    <body-content>tagdependent</body-content>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <description><![CDATA[任务办理按钮]]></description>
    <name>workflowButtonGroup</name>
    <tag-class>com.norteksoft.tags.workflow.WorkflowButtonGroupTag</tag-class>
    <body-content>tagdependent</body-content>
    <attribute>
      <description><![CDATA[任务id]]></description>
      <name>taskId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[表单的名称]]></description>
      <name>formCode</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[表单的版本]]></description>
      <name>version</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[按钮事件对象名，默认为workflowButtonGroup]]></description>
      <name>name</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <description><![CDATA[是否显示"抄送"和"指派"按钮]]></description>
      <name>showOtherButton</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
</taglib>
